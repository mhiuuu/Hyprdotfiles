{
    "custom/launcher": {
        "format": "󰌽",
        "on-click": "rofi show -drun"
    },
    "custom/texttwo": {
        "exec": "echo '󰉓   File'",
        "interval": 60,
        "return-type": "plain",
        "on-click": "Thunar"
    },
    "custom/textthree": {
        "exec": "echo '󰨞  Code'",
        "interval": 60,
        "return-type": "plain",
        "on-click": "code"
    },
    "custom/textfour": {
        "exec": "echo '  Terminal'",
        "interval": 60,
        "return-type": "plain",
        "on-click": "kitty"
    },
    "custom/textfive": {
        "exec": "echo '󰇩  Browser'",
        "interval": 60,
        "return-type": "plain",
        "on-click": "microsoft-edge-stable"
    },
    "hyprland/window": {
        "format": "{class}",
        "max-length": 20,
        "icon": true,
        "icon-size": 15,
        "rewrite": {
            "^(?!.*\\S).*": " Nothing"
        }
    },
    "custom/spotify": {
        "interval": 1,
        "return-type": "json",
        "exec": "~/.config/waybar/modules/spotify.sh",
        "exec-if": "pgrep spotify",
        "escape": true
    }
}
